#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,count1=0,count2=0;
	    cin>>n;
	    string s;
	    cin>>s;
	    
	    if(s[0]=='A')
	    {
	        count1++;
	    }
	    for(int i=0;i<s.size();i++)
	    {
	        if(s[i]=='A'&& s[i+1]=='A')
	        {
	            count1++;
	        }
	       else
	         if(s[i]=='B'&& s[i+1]=='B')
	         {
	             count2++;
	         }
	    }
	    cout<<count1<<" "<<count2<<endl;
	}
	return 0;
